{
  "name": "time_tracker_front",
  "version": "1.0.0",
  "description": "The frontend for time tracker app",
  "author": "Andres Sild",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 3001",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test:unit": "cross-env TEST=unit nyc ava --config unit-tests.config.js  --color",
